    "@types/react-dom": "^18.2.18",
    "@types/react-modal": "^3.16.3",
    "@types/uuid": "^9.0.8",
    "prisma": "^5.9.0",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
