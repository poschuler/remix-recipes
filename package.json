    "@remix-run/react": "^2.5.1",
    "@remix-run/serve": "^2.5.1",
    "@remix-run/v1-route-convention": "^0.1.4",
    "cross-env": "^7.0.3",
    "cryptr": "^6.3.0",
    "form-data": "^4.0.0",
    "isbot": "^4.4.0",
    "mailgun.js": "^10.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-modal": "^3.16.1",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
